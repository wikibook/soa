<?xml version="1.0" encoding="UTF-8"?>
<soajAdaptiveConfiguration xmlns="http://javector.com/ser/adaptive/handler" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<HandlerMappings>
		<AdaptiveContextHandler>
			<OverrideAdaptiveContextHandler>com.javector.adaptive.jaxb.impl.AdaptiveJaxbContext</OverrideAdaptiveContextHandler>
		</AdaptiveContextHandler>
		<UtilHandlers>
			<OverrideXpathEvaluatorHandler>com.javector.adaptive.jaxb.util.JaxBXPathEvaluator</OverrideXpathEvaluatorHandler>
			<OverrideReflectionHandler>com.javector.adaptive.jaxb.util.JAXBReflectionUtil</OverrideReflectionHandler>
		</UtilHandlers>
		<SerializerHandlers>
			<OverrideBaseAdaptiveSerializerHandler>com.javector.adaptive.framework.interfaces.BaseAdaptiveSerializer</OverrideBaseAdaptiveSerializerHandler>
			<OverrideSimpleTypeSerializerHandler>com.javector.adaptive.jaxb.impl.JAXBSimpleTypeSerializer</OverrideSimpleTypeSerializerHandler>
			<OverrideComplexTypeSerializerHandler>com.javector.adaptive.jaxb.impl.JAXBComplexTypeSerializer</OverrideComplexTypeSerializerHandler>
		</SerializerHandlers>
		<DeserializerHandlers>
			<OverrideBaseDeserializerHandler>com.javector.adaptive.jaxb.impl.JAXBComplexTypeDeserializer</OverrideBaseDeserializerHandler>
			<OverrideComplexTypeDeserializerHandler>com.javector.adaptive.jaxb.impl.JAXBComplexTypeDeserializer</OverrideComplexTypeDeserializerHandler>
			<OverrideSimpleTypeDeserializerHandler>com.javector.adaptive.jaxb.impl.JAXBSimpleTypeDeserializer</OverrideSimpleTypeDeserializerHandler>
		</DeserializerHandlers>
	</HandlerMappings>
</soajAdaptiveConfiguration>
