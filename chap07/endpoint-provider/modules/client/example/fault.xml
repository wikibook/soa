  xmlns:ns1="http://www.example.com/req" xmlns:ns2="http://www.example.com/oms"
  xmlns:ns3="http://www.example.com/faults">
  <soapenv:Body>
    <soapenv:Fault xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/">
